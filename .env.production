# F1 Data Explorer - Production Environment Configuration
# ======================================================

# API Configuration
VITE_F1_API_BASE_URL=https://api.jolpi.ca/ergast/f1
VITE_F1_API_FORMAT=json
VITE_F1_API_TIMEOUT=30000
VITE_F1_API_RETRIES=3

# Rate Limiting Configuration
VITE_F1_API_MAX_CONCURRENT_REQUESTS=3
VITE_F1_API_DELAY_BETWEEN_REQUESTS=500
VITE_F1_API_RETRY_DELAY_MULTIPLIER=2

# Cache Configuration
VITE_F1_CACHE_DURATION=300000
VITE_F1_CACHE_MAX_SIZE=100

# Application Configuration
VITE_F1_DEFAULT_SEASON=2024
VITE_F1_DEFAULT_ROUND=1
VITE_F1_APP_NAME=F1 Data Explorer
VITE_F1_APP_VERSION=1.0.0

# UI Configuration
VITE_F1_DEFAULT_THEME=light
VITE_F1_ENABLE_ANIMATIONS=true
VITE_F1_SIDEBAR_COLLAPSED=false
VITE_F1_TABLE_PAGE_SIZE=20

# Feature Flags
VITE_F1_ENABLE_RACE_RESULTS=true
VITE_F1_ENABLE_QUALIFYING_RESULTS=true
VITE_F1_ENABLE_SPRINT_RESULTS=false
VITE_F1_ENABLE_LAP_TIMES=false
VITE_F1_ENABLE_PIT_STOPS=false
VITE_F1_ENABLE_CIRCUITS=true
VITE_F1_ENABLE_HISTORICAL_DATA=true

# Production Configuration
VITE_F1_DEBUG_MODE=false
VITE_F1_LOG_LEVEL=error
VITE_F1_ENABLE_CONSOLE_LOGS=false

# Performance Configuration
VITE_F1_ENABLE_LAZY_LOADING=true
VITE_F1_PREFETCH_NEXT_SEASON=true
VITE_F1_ENABLE_SERVICE_WORKER=true

# External Services
VITE_F1_ANALYTICS_ID=
VITE_F1_SENTRY_DSN=
VITE_F1_FEEDBACK_URL=

# F1 Services Configuration - Production URLs
VITE_F1_MCP_SERVER_URL=https://f1-mcp-server-5dh3.onrender.com
VITE_F1_LANGGRAPH_AGENTS_URL=https://f1-sequential-agents.onrender.com