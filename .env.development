# F1 Data Explorer - Development Environment Configuration
# ==============================================
# This file overrides .env for development purposes

# API Configuration - Development
VITE_F1_API_BASE_URL=https://api.jolpi.ca/ergast/f1
VITE_F1_API_FORMAT=json
VITE_F1_API_TIMEOUT=45000
VITE_F1_API_RETRIES=5

# Rate Limiting Configuration - More lenient for development
VITE_F1_API_MAX_CONCURRENT_REQUESTS=2
VITE_F1_API_DELAY_BETWEEN_REQUESTS=1000
VITE_F1_API_RETRY_DELAY_MULTIPLIER=2

# Cache Configuration - Shorter cache for development
VITE_F1_CACHE_DURATION=60000
VITE_F1_CACHE_MAX_SIZE=50

# Application Configuration
VITE_F1_DEFAULT_SEASON=2024
VITE_F1_DEFAULT_ROUND=1
VITE_F1_APP_NAME=F1 Data Explorer (Dev)
VITE_F1_APP_VERSION=1.0.0-dev

# UI Configuration
VITE_F1_DEFAULT_THEME=light
VITE_F1_ENABLE_ANIMATIONS=true
VITE_F1_SIDEBAR_COLLAPSED=false
VITE_F1_TABLE_PAGE_SIZE=10

# Feature Flags - Enable more features in development
VITE_F1_ENABLE_RACE_RESULTS=true
VITE_F1_ENABLE_QUALIFYING_RESULTS=true
VITE_F1_ENABLE_SPRINT_RESULTS=true
VITE_F1_ENABLE_LAP_TIMES=true
VITE_F1_ENABLE_PIT_STOPS=true
VITE_F1_ENABLE_CIRCUITS=true
VITE_F1_ENABLE_HISTORICAL_DATA=true

# Development Configuration - Enable all debugging
VITE_F1_DEBUG_MODE=true
VITE_F1_LOG_LEVEL=debug
VITE_F1_ENABLE_CONSOLE_LOGS=true

# Performance Configuration
VITE_F1_ENABLE_LAZY_LOADING=false
VITE_F1_PREFETCH_NEXT_SEASON=false
VITE_F1_ENABLE_SERVICE_WORKER=false

# MCP Server Configuration - Local development
VITE_F1_MCP_SERVER_URL=http://localhost:3001
VITE_F1_LANGGRAPH_AGENTS_URL=http://localhost:3002
